<section>
	<h4> <%= @company.name %> </h4>
	<p> Phone Number: <%= @company.phone_number %> </p>
	<p> <%= @company.homepage_url %> </p>
	<p> Funds Raised: <%= @company.total_money_raised %> </p>
	<p> <%= raw @company.overview %> </p>
	<% acquisitions = @company.acquisitions %>
	<% acquisitions_hash = Hash[acquisitions.map {|key, value| [key, value]}] %>
	<p> Acquisitions: <%= acquisitions_hash.class %> </p>
	<p> <%= acquisitions_hash %> </p>
</section>